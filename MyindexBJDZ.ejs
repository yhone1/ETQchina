    <div class="myDivRight">
        <div class="myDivRight_GG">
            <h1>收件地址</h1>
            <div class="myDivRight_GG_1 dizhi" >

                <div class="daimabiji-container">
                    <div class="container-fluid">
                        <form class="form-inline" action="/dizhi.do" method="post">
                            <div class="input ">
                                <p>收件人姓名*</p>
                                <input type="text" placeholder="长度不超过10个字符" name="addname">
                            </div>
                            <div class="input ">
                                <p>号码*</p>
                                <input type="text" placeholder="电话号码,手机号码必须填一项" name="addphone">
                            </div>
                            <div id="distpicker3" class="input ">
                                <p>地址*</p>
                                <div class="form-group">
                                    <label class="sr-only" for="province6">Province</label>
                                    <select class="form-control" id="province6" name="addsheng"></select>
                                </div>
                                <div class="form-group">
                                    <label class="sr-only" for="city6">City</label>
                                    <select class="form-control" id="city6" name="addshi"></select>
                                </div>
                                <div class="form-group">
                                    <label class="sr-only" for="district6">District</label>
                                    <select class="form-control" id="district6" name="addxian"></select>
                                </div>
                            </div>
                            <div class="input ">
                                <p>街道详细地址*</p>
                                <input type="text" placeholder="请输入详细地址信息，如道路、门牌号、小区、楼栋号、单元等信息" name="addjiedao">
                            </div>
                            <div class="input ">
                                <p>邮政编码*</p>
                                <input type="text" placeholder="请填写邮编" name="addyoubian">
                            </div>
                            <div class="input button">
                                <button type="submit">保存地址</button>
                            </div>
                        </form>
                    </div>
                </div><!--联动-->
            </div>
        </div>
